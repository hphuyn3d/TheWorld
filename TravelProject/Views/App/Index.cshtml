@model IEnumerable<TravelProject.Models.Trip>
@{
    ViewBag.Title = "Home Page";
}


<div class="row">
    <div class="col-md-6">
        <h3>The World</h3>
        <p>This will be aA fun website soon</p>
        <form>
            <div class="form-group">
                <label>Date</label>
                <input class="form-control" />
            </div>
            <div>
                <label>Location</label>
                <input class="form-control" />
            </div>
            <div><hr/>
                <input type="submit" value="Add" class="btn btn-success" />
            </div>
        </form>
    </div>
    <div class="col-md-6">
        <h2>The Map</h2>
        <ul>
            @foreach (var item in Model)
            {
                <li>@item.Name : @item.DateCreated.ToString("d")</li>
            }
         
        </ul>
    </div>
</div>